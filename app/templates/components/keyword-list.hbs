<ul>
    {{#each keywords as |keyword|}}
        <li>
            {{#link-to 'keyword' keyword class='name'}}
                <span>{{ keyword.id }} ({{ format-num keyword.crates_cnt }})</span>
                <img class="right-arrow" src="/assets/right-arrow.png"/>
            {{/link-to}}
        </li>
    {{/each}}
</ul>
